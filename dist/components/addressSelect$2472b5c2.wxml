<template name="addressSelect$2472b5c2">
  <view class="_div data-v-f4ec123e address">
    <scroll-view scroll-y="true" class="_scroll-view data-v-f4ec123e addcont" style="height: 100%;">
      <view wx:if="{{listData.length!=0}}" class="_div data-v-f4ec123e item">
        <view class="_div data-v-f4ec123e list" wx:key="index" key="{{index}}" wx:for="{{listData}}" wx:for-index="index" wx:for-item="item">
          <view class="_div data-v-f4ec123e addresslist" style=" {{item.textStyle}}">
            <view class="_div data-v-f4ec123e"><label class="_span data-v-f4ec123e">{{item.name}}</label>
              <view wx:if="{{item.is_default}}" class="_div data-v-f4ec123e moren"> 默认 </view>
            </view>
            <view bindtap="handleProxy" class="_div data-v-f4ec123e info" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}">
              <view class="_p data-v-f4ec123e">{{item.mobile}}</view>
              <view class="_p data-v-f4ec123e">{{item.address+item.address_detail}}</view>
            </view>
            <view bindtap="handleProxy" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}" class="_div data-v-f4ec123e"></view>
          </view>
        </view>
      </view>
      <view wx:else class="_div data-v-f4ec123e center">
        <view class="_p data-v-f4ec123e">收货地址在哪里?</view>
      </view>
    </scroll-view>
    <view class="_div data-v-f4ec123e bottom">
      <view bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_div data-v-f4ec123e">+新建地址</view>
      <view bindtap="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_div data-v-f4ec123e">一键导入微信地址</view>
    </view>
  </view>
</template>